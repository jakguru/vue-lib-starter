    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.89.2",
    "shiki": "^3.7.0",
    "terser": "^5.43.1",
    "typedoc": "^0.28.7",
    "typedoc-plugin-markdown": "^4.7.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
