    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.87.0",
    "shiki": "^3.4.0",
    "terser": "^5.39.0",
    "typedoc": "^0.28.2",
    "typedoc-plugin-markdown": "^4.6.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
